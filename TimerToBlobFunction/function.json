{
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */5 * * * *"
    },
    {
      "name": "outputBlob",
      "type": "blob",
      "path": "expired-products/{datetime}.json",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ]
}
